{
    // "image": "devcontainer:latest",
    "image": "nyhavna:latest",
    "name": "mycontainer",
    "customizations": {
        "vscode": {
            "extensions": [
                "bradlc.vscode-tailwindcss",
                "donjayamanne.githistory",
                "editorconfig.editorconfig",
                "github.copilot",
                "janisdd.vscode-edit-csv",
                "kevinrose.vsc-python-indent",
                "mhutchie.git-graph",
                "modular-mojotools.vscode-mojo",
                "ms-azuretools.vscode-docker",
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-toolsai.jupyter",
                "ms-vscode-remote.remote-ssh",
                "ms-vscode.cpptools-extension-pack",
                "ms-vscode.cpptools",
                "nvidia.nsight-vscode-edition",
                "svelte.svelte-vscode",
                "visualstudioexptteam.vscodeintellicode",
                "znck.grammarly",
                "james-yu.latex-workshop",
            ]
        }
    },
    "runArgs": [
        "--gpus=all",
        "--net=host",
        "-eNVIDIA_DRIVER_CAPABILITIES=all",
        // "--add-host=host.docker.internal:host-gateway",
        // "--cap-add=SYS_PTRACE",
        // "--security-opt",
        // "seccomp=unconfined",
        // "--device=/dev/ACM0"
        // "${localEnv:DEVCONTAINER_RUN_ARGS}"
    ],
    "forwardPorts": [
        5173, // SvelteKit
        5170, // WebSockets
    ],
    // "postStartCommand": "cd ${containerWorkspaceFolder}/.devcontainer && python3 post_start.py",
    // "initializeCommand": "cd ${localWorkspaceFolder}/.devcontainer && python3 initialize.py",
    // "workspaceFolder": "/workspaces"
    // "workspaceMount": "source=sensorrig-ws,target=/workspace,type=volume",
}